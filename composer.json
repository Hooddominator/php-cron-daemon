{
    "name": "wiryonolau/php-cron-daemon",
    "description": "PHP daemon for script and cronjob",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "zf2",
        "zend framework 2"
    ],
    "homepage": "https://github.com/wiryonolau/php-cron-daemon",
    "authors": [
        {
            "name": "Wiryono Lau",
            "email": "wiryonolau@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "zfcampus/zf-console": "^1.4",
        "zendframework/zend-servicemanager": "^3.3",
        "vanushwashere/cron-expression-php5": "v1.2.0.3"
    },
    "autoload": {
        "psr-4": {
            "CronDaemon\\": "module/CronDaemon/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CronDaemonTest\\": "module/CronDaemon/test/"
        }
    },
    "require-dev": {
        "zendframework/zend-coding-standard": "^1.0",
        "phpunit/phpunit": "^5.7",
        "zendframework/zend-hydrator": "^2.3",
        "zendframework/zend-filter": "^2.7"
    }
}
